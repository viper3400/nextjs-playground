{
  "name": "jaxx-next-play",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "typedoc": "typedoc --logLevel Verbose",
    "typedoc:md": "typedoc --plugin typedoc-plugin-markdown",
    "test:coverage": "jest --coverage",
    "test": "jest unit",
    "test:integration": "jest integration",
    "clean": "rimraf dist target",
    "serenity-bdd:update": "serenity-bdd update",
    "test:ct": "failsafe clean serenity-bdd:update test:execute test:report",
    "test:execute": "playwright test -c playwright-ct.config.ts",
    "test:report": "serenity-bdd run --features ./src/stories"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.5.2",
