    "next": "14.2.2",
    "next-auth": "^4.24.7",
    "prisma": "^5.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@mswjs/data": "^0.16.1",
    "@playwright/experimental-ct-react": "^1.42.1",
